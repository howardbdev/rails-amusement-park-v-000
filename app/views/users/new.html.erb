users/new <br>

<h1>Please Sign Up:</h1>

<%= form_for @user do |f| %>
  <%= f.label :name  %>
  <%= f.text_field :name %><br>
  <%= f.label :password  %>
  <%= f.text_field :password %><br>
  <%= f.label :tickets  %>
  <%= f.text_field :tickets %><br>
  <%= f.label :nausea  %>
  <%= f.text_field :nausea %><br>
  <%= f.label :happiness  %>
  <%= f.text_field :happiness %><br>
  <%= f.label :height  %>
  <%= f.text_field :height %><br>
  <%= f.label :admin  %>
  <%= f.text_field :admin %><br>
  <%= f.submit 'Create User' %>
<% end %>
