attractions/index <br>

<ul>
<% @attractions.each do |attraction| %>
  <li>
    <ul>
      <h3><%= attraction.name %></h3>
      <li>Ticket Requirement: <%= attraction.tickets %></li>
      <li>Height Requirement: <%= attraction.min_height %></li>
      <li>Nausea Rating: <%= attraction.nausea_rating %></li>
      <li>Happiness Rating: <%= attraction.happiness_rating %></li>
      <% if @user.admin %>
        <%= link_to "Show #{attraction.name}", attraction_path(attraction) %>
      <% else %>
        <%= link_to "Go on #{attraction.name}", attraction_path(attraction) %>
      <% end %>
    </ul>
  </li>
<% end %>
</ul>

<%= link_to "New Attraction", new_attraction_path if @user.admin %>
